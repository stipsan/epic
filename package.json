{
  "name": "epic",
  "version": "1.0.0",
  "description": "React example project, that takes you from fun development to high quality production",
  "scripts": {
    "bundle": "NODE_ENV=production webpack",
    "start": "babel-node lib/server/web",
    "start-production": "NODE_ENV=production node lib/server/web && npm run bundle -- --watch",
    "postinstall": "cd assets/exports && node ../../scripts/assets && cd ../.. && npm run favicon",
    "heroku-postbuild": "npm run bundle && npm run babel",
    "hmr": "webpack-dev-server",
    "test": "NODE_ENV=test mocha --compilers js:./test/babel-register.js,js:./test/css-modules-compiler.js --recursive -t 20000",
    "test:watch": "npm test -- --watch --growl",
    "coveralls": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -t 20000 --compilers js:./test/css-modules-compiler.js -R spec --recursive test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "reboot-workers": "kill -SIGUSR2",
    "favicon": "cd public/favicons && convert icon-16.png icon-32.png icon-96.png ../favicon.ico",
    "babel": "npm run babel-server && npm run babel-shared",
    "babel:watch": "npm run babel-server -- --watch && npm run babel-shared -- --watch",
    "babel-server": "babel src/server --out-dir lib/server --no-babelrc --plugins transform-es2015-modules-commonjs,transform-es2015-parameters,transform-es2015-spread,transform-es2015-destructuring,transform-object-rest-spread",
    "babel-shared": "babel src/shared --out-dir lib/shared --no-babelrc --plugins transform-es2015-modules-commonjs,transform-es2015-parameters,transform-es2015-spread,transform-es2015-destructuring,transform-object-rest-spread"
  },
  "engines": {
    "node": "6.2.0",
    "npm": "3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/epic.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "flux",
    "redux",
    "example",
    "game",
    "socketcluster",
    "heroku",
    "immutable",
    "redis",
    "ioredis"
  ],
  "author": "Stian Didriksen <stipsan@gmail.com> (http://github.com/stipsan)",
  "meta": {
    "author": "Stian Didriksen",
    "description": "Epic is a fun and addicting two-player game. Hide your items well and take turns searching each other in a race to 21 points!"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/epic/issues"
  },
  "homepage": "https://github.com/stipsan/epic#readme",
