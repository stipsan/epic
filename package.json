{
  "name": "epic",
  "version": "1.0.0",
  "description": "React example project, that takes you from fun development to high quality production",
  "scripts": {
    "bundle": "NODE_ENV=production webpack",
    "start": "concurrently 'babel-node src/server/web' 'npm run hmr' 'npm run remotedev' --kill-others",
    "nasa": "webpack-dashboard -- npm run hmr",
    "start-production": "NODE_ENV=production node lib/server/web && npm run bundle -- --watch",
    "postinstall": "cd assets/exports && node ../../scripts/assets && cd ../.. && npm run favicon",
    "heroku-postbuild": "npm run bundle && npm run babel",
    "hmr": "webpack-dev-server",
    "remotedev": "remotedev --hostname=localhost --port=8000",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "coveralls": "cat .coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "codeclimate": "codeclimate-test-reporter < .coverage/lcov.info",
    "coverage": "npm run coveralls && npm run codeclimate",
    "favicon": "cd public/favicons && convert icon-16.png icon-32.png icon-96.png ../favicon.ico",
    "babel": "BABEL_ENV=server NODE_ENV=production babel src --out-dir lib",
    "babel:watch": "npm run babel -- --watch"
  },
  "engines": {
    "node": "6.4.0",
    "npm": "3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/epic.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "flux",
    "redux",
    "example",
    "game",
    "socketcluster",
    "heroku",
    "immutable",
    "redis",
    "ioredis"
  ],
  "author": "Stian Didriksen <stipsan@gmail.com> (http://github.com/stipsan)",
  "meta": {
    "author": "Stian Didriksen",
    "description": "Epic is a fun and addicting two-player game. Hide your items well and race to the highest score!"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/epic/issues"
  },
  "jest": {
    "automock": false,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/",
      "<rootDir/src/client/index.js"
    ],
    "coverageDirectory": "<rootDir>/.coverage",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "epic-client(.*)": "<rootDir>/src/client$1.js",
      "^.+\\.(css|less)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    }
  },
  "homepage": "https://github.com/stipsan/epic#readme",
  "dependencies": {
