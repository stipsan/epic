{
  "name": "epic",
  "version": "1.0.0",
  "description": "Just a simple game to learn the ropes of redux, socket.io and sharpen my skills on react, js and node development in general",
  "scripts": {
    "bundle": "NODE_ENV=production webpack",
    "start": "nf start web=1",
    "start-production": "NODE_ENV=production node server/web && npm run bundle -- --watch",
    "heroku-postbuild": "npm run bundle",
    "hmr": "webpack-dev-server",
    "test": "NODE_ENV=test mocha --compilers js:babel-register,js:./test/css-modules-compiler.js --recursive -t 10000",
    "test:watch": "npm test -- --watch",
    "coveralls": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -t 10000 --compilers js:./test/css-modules-compiler.js -R spec --recursive test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "reboot-workers": "kill -SIGUSR2"
  },
  "engines": {
    "node": "5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/epic.git"
  },
  "author": "Stian Didriksen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stipsan/epic/issues"
  },
  "homepage": "https://github.com/stipsan/epic#readme",
  "dependencies": {
    "@stipsan/express-history-api-fallback": "2.1.0",
    "assets-webpack-plugin": "3.4.0",
